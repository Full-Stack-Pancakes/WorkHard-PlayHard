<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.pojos.User">
		<id name="id" column="USERID">
			<generator class="increment"></generator>
		</id>
		<property name="firstname" type="org.hibernate.type.StringType" column="FIRSTNAME"></property>
		<property name="lastname" type="org.hibernate.type.StringType" column="LASTNAME"></property>
		<property name="email" type="org.hibernate.type.StringType" column="EMAIL"></property>
		<property name="phone" type="org.hibernate.type.StringType" column="PHONE"></property>
		<property name="password" type="org.hibernate.type.StringType" column="PASSWORD"></property>
	</class>
</hibernate-mapping>